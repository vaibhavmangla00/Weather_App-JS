<%- include ("header") -%>

<body class="weather text-center">

    <div>
        <h1 class="weather-heading">
            The weather is currently <%= description %>
        </h1>
    </div>
    <div>
        <h2 class="weather-desc">
            The temperature in <%= city %> is <%= temp %><span>&#176;C</span>
        </h2>
    </div>
    <div>
        <img class="weather-image" src=<%= imageurl %>>
    </div>
</body>


<%- include ("footer") -%>